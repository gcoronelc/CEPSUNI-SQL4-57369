  <DataSets>
    <DataSet Name="DS_PRINCIPAL">
      <Query>
        <DataSourceName>DS57369NWDIM</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ANIO">
            <Value>=Parameters!ANIO.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CATEGORIA">
            <Value>=Parameters!CATEGORIA.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT     CLIENTE.paisCliente, VENTAS_FACT.unidades, VENTAS_FACT.monto, VENTAS_FACT.fecha, PRODUCTO.idCategoria
FROM        CLIENTE INNER JOIN
                  VENTAS_FACT ON CLIENTE.idCliente = VENTAS_FACT.idCliente INNER JOIN
                  PRODUCTO ON VENTAS_FACT.idProducto = PRODUCTO.idProducto
WHERE     (YEAR(VENTAS_FACT.fecha) IN (@ANIO))
AND PRODUCTO.idCategoria = @CATEGORIA</CommandText>
      </Query>
      <Fields>
        <Field Name="paisCliente">
          <DataField>paisCliente</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="unidades">
          <DataField>unidades</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="monto">
          <DataField>monto</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="fecha">
          <DataField>fecha</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="idCategoria">
          <DataField>idCategoria</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DS_Anios">
      <Query>
        <DataSourceName>DS57369NWDIM</DataSourceName>
        <CommandText>select DISTINCT YEAR(VENTAS_FACT.fecha) ANIO FROM VENTAS_FACT order by 1 desc</CommandText>
      </Query>
      <Fields>
        <Field Name="ANIO">
          <DataField>ANIO</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DS_Categorias">
      <Query>
        <DataSourceName>DS57369NWDIM</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ANIO2">
            <Value>=Parameters!ANIO.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>WITH
PROD AS (
	SELECT DISTINCT idProducto FROM VENTAS_FACT V
	WHERE YEAR(fecha) = @ANIO2
)
SELECT DISTINCT P.idCategoria, P.nomCategoria 
FROM PRODUCTO P
JOIN PROD ON P.idProducto = PROD.idProducto</CommandText>
      </Query>
      <Fields>
        <Field Name="idCategoria">
          <DataField>idCategoria</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="nomCategoria">
          <DataField>nomCategoria</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
